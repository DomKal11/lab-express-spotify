<div class="container tiles">
  {{#each tracks}}
    <div class="card">
      <h5 class="card-title">{{this.name}}</h5>
      <audio controls src="{{this.preview_url}}">
        Your browser does not support the
        <code>audio</code>
        element.
      </audio>
    </div>
  {{/each}}
</div>